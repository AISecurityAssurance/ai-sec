# Example .env file for STPA-Sec Analysis
# Copy this to .env and fill in your actual values

# Azure OpenAI
AZURE_OPENAI_API_KEY=your-azure-api-key-here
AZURE_OPENAI_BASE_URL=https://your-resource.openai.azure.com/
AZURE_OPENAI_MODEL=gpt-4o

# Standard OpenAI
OPENAI_API_KEY=sk-your-openai-key-here
OPENAI_MODEL=gpt-4-turbo

# Anthropic
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here
ANTHROPIC_MODEL=claude-3-opus-20240229

# Groq
GROQ_API_KEY=gsk_your-groq-key-here
GROQ_MODEL=llama2-70b-4096

# Custom endpoints (for self-hosted models)
CUSTOM_BASE_URL=http://localhost:8080
CUSTOM_API_KEY=your-custom-key

# Model selection (for configs that use model_env)
MY_PREFERRED_MODEL=gpt-4o
MY_PREFERRED_PROVIDER=openai